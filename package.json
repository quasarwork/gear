{
  "name": "quasarwork-gear",
  "version": "0.0.1",
  "private": true,
  "description": "Gear project monorepo.",
  "homepage": "https://github.com/quasarwork/gear",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quasarwork/gear"
  },
  "license": "MIT",
  "author": {
    "name": "AurÃ©lien BOBENRIETH",
    "url": "https://quasar.work"
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "changeset:publish": "yarn tidy && yarn changeset version && changeset publish",
    "format:check": "turbo run format:check",
    "format:write": "turbo run format:write",
    "postinstall": "husky",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test": "turbo run test",
    "test:ut": "turbo run test:ut",
    "tidy": "turbo run tidy",
    "types:check": "turbo run types:check"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md,yml,yaml}": [
      "turbo run tidy --",
      "turbo run test:ut --"
    ]
  },
  "prettier": "@quasarwork/gear-prettier-config",
  "dependencies": {
    "@changesets/cli": "^2.27.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@changesets/changelog-github": "^0.5.0",
    "@commitlint/cli": "^19.4.1",
    "@quasarwork/gear-prettier-config": "*",
    "@quasarwork/gear-tsconfig": "*",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "pinst": "^3.0.0",
    "rimraf": "^6.0.1",
    "turbo": "^2.0.14",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=16.0.0"
  }
}
